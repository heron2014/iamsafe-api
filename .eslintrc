{
  "plugins": ["import"],
  "extends": [
    "plugin:import/errors",
    "plugin:import/warnings",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "no-console": ["warn", {allow: ["warn","error"]}],
    "no-unused-vars": 0
  }
}
